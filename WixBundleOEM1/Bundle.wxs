<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Bundle Name="云合景从" Version="1.0.20.0" Manufacturer="合肥新云晖科技有限公司" UpgradeCode="c76fa141-5ab1-411d-8277-e53e116d8b9b" IconSourceFile="icon.ico" DisableModify="yes">
    <BootstrapperApplicationRef Id="ManagedBootstrapperApplicationHost"   >
      <Payload SourceFile="$(var.CustomBAOEM1.TargetDir)CustomBA.dll" />
      <Payload SourceFile="$(var.CustomBAOEM1.TargetDir)BootstrapperCore.config" />
    </BootstrapperApplicationRef>

    <Chain>
      <PackageGroupRef Id="Netfx4Full"/>
     
      <MsiPackage SourceFile="$(var.SetupOEM1.TargetPath)">
        <MsiProperty Name="INSTALLFOLDER" Value="[InstallFolder]"/>
      </MsiPackage>
    </Chain>
  </Bundle>

  <Fragment>
    <PackageGroup Id="Netfx4Full">
      <ExePackage Id="Netfx4Full" Cache="no" Compressed="yes" PerMachine="yes" Permanent="yes" Vital="yes" InstallCommand=" /q /norestart"
                  SourceFile="dotNetFx40_Full_setup.exe" DetectCondition="DotNetFramework40FullInstallRegValue=1" />
    </PackageGroup>
    <WixVariable Id="WixMbaPrereqPackageId" Value="Netfx4Full" />
    <WixVariable Id="WixMbaPrereqLicenseUrl" Value="NetfxLicense.rtf" />
  </Fragment>

</Wix>
